--First SQL Code
--Create a store database

CREATE TABLE mystore (id INTEGER PRIMARY KEY, name TEXT, quantity INTEGER, rating TEXT, price INTEGER);

INSERT INTO mystore VALUES 
(1, 'shirt', 30, '100%', 5),
(2, 'pants', 50, '95%', 12),
