(12, 'hats', 120, '87%', 45),
(13, 'gloves', 220, '67%', 89),
(14, 'wallets', 32, '100%', 9),
(15, 'watches', 22, '100%', 66)
;

--Displaying items from lowest to highest price

SELECT * FROM mystore;
SELECT * FROM mystore ORDER BY price ASC;
